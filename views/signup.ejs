                <figure>
                    <img src="/Group 1000005941img.png" alt="">
                </figure>
            </div>


            <div>

                <form class="login-form" action="#" method="POST">
                    <h3>Create an account</h3>
                    <p class="margin-bottom">Enter your details below</p>

